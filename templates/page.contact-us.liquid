  <div class="page-wrapper">
    <div class="main-page">
      <div class="form-section">
        <div class="side-banner"></div>
        <div class="form-container">
          <div class="form-wrapper-2">
            <h1 class="form-heading">{{ 't7f274abe' | t }}</h1>
            <div class="contact-text-2">{{ 'tn7cd7da67' | t }}</div>
            <div class="form-paragraph">{{ 'tn6951bd8f' | t }}<br></div>
            <div class="form-block w-form">
              {%- form "contact",  class: "" -%}
                <div class="form-section-wrap">
                  <div class="w-layout-grid form-grid">
                    <div class="grid-cell"><label for="contact[first_name]" class="field-label">{{ 't24eeab' | t }}</label><input class="text-field w-input" maxlength="256" name="contact[first_name]" data-name="First Name" placeholder="{{ 't24eeab' | t }}" type="text" id="First-Name"></div>
                    <div class="grid-cell"><label for="Email-Form" class="field-label">{{ 't3ff5b7c' | t }}</label><input class="text-field w-input" maxlength="256" name="contact[email]" data-name="Email" placeholder="{{ 't3ff5b7c' | t }}" type="email" id="Email-Form"></div>
                    <div class="grid-cell"><label for="contact[detail]" class="field-label">{{ 'tn63dc6819' | t }}</label><input class="text-field w-input" maxlength="256" name="contact[detail]" data-name="Detail" placeholder="{{ 'tn7fc0aae6' | t }}" type="text" id="Detail"></div>
                  </div>
                </div><input type="submit" data-wait="Please wait..." class="button-primary w-button" value="{{ 'tn4035be09' | t }}">
              {%- assign form_success = form.posted_successfully? -%}{%- assign form_errors = form.errors -%}{%- render "form-general-script", form_success: form_success, form_errors: form_errors -%}{%- endform -%}
              <div class="success-message w-form-done  {% if form_success %}form-posted-successfully{% endif %}">
                <div>{{ 't5a0ea5a1' | t }}</div>
              </div>
              {%- unless form_success -%}<div class="w-form-fail  {% if form.errors %}form-has-errors{% endif %}">
                <div>{%- render "form-errors", form_errors: form_errors -%}</div>
              </div>{%- endunless -%}
            </div>
            <div class="form-paragraph builder-text">{{ 't68d81a32' | t }}<br></div>
          </div>
        </div>
      </div>
    </div>
  </div>
