{% capture listItems %}
{% paginate collections.all.products by 999 %}
{%- assign allItems = collections.all.products -%}
{%- for item in allItems  -%}
                <div role="listitem" class="w-dyn-item">
                  <div class="content-card-l">
                    <div class="content-top-detail">
                      <div class="paragraph-small-14 text-weight-bold">{{ item.metafields.udesly['selling-point'] }}</div>
                      <div class="content-tag-wrap">
                        <div class="paragraph-small-14 text-weight-bold">Pet Home</div>
                        <div class="wrap-h-x-small"></div>
                      </div>
                    </div>
                    <div class="content-image-v3"><img loading="lazy" src="{{ item.featured_image | img_url: 'master' }}" alt="{{ item.featured_image.alt }}" class="image-cover-10" data-commerce-type="variation-image" srcset="{{ item.featured_image | img_url: '1920x' }} 1920w,{{ item.featured_image | img_url: '1600x' }} 1600w,{{ item.featured_image | img_url: '1366x' }} 1366w,{{ item.featured_image | img_url: '1024x' }} 1024w,{{ item.featured_image | img_url: '768x' }} 768w,{{ item.featured_image | img_url: '640x' }} 640w"></div>
                    <div>
                      <div class="wrap-v-regular">
                        <h4 class="h4-heading-3">{{ item.title }}</h4>
                      </div>
                      <div class="content-bottom-detail">
                        <a href="{{ item.url }}" class="button-link-large w-inline-block">
                          <div class="icon-link">
                            <div class="icon-small-3 w-embed"><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M3.47233 8.66657H10.919L7.66566 11.9199C7.40566 12.1799 7.40566 12.6066 7.66566 12.8666C7.92566 13.1266 8.34566 13.1266 8.60566 12.8666L12.999 8.47323C13.259 8.21323 13.259 7.79323 12.999 7.53323L8.61233 3.13323C8.35233 2.87323 7.93233 2.87323 7.67233 3.13323C7.41233 3.39323 7.41233 3.81323 7.67233 4.07323L10.919 7.33323H3.47233C3.10566 7.33323 2.80566 7.63323 2.80566 7.9999C2.80566 8.36657 3.10566 8.66657 3.47233 8.66657Z" fill="currentColor"></path>
                              </svg></div>
                          </div>
                          <div class="text-block-55">Shop Now</div>
                        </a>
                        <a data-commerce-type="variation-price" href="#" class="client-link-2">{{ item.price | money_with_currency }}</a>
                      </div>
                    </div>
                  </div>
                </div>
{%- endfor -%}
{% endpaginate %}
{% endcapture %}
{% assign items_length = listItems | strip | size %}

<div class="w-dyn-list" udy-collection="product">
              {%- unless items_length == 0 -%}<div role="list" class="collection-list-2 w-dyn-items">
                {{ listItems }}
              </div>{%- endunless -%}
              {%- if items_length == 0 -%}<div class="w-dyn-empty">
                <div>No items found.</div>
              </div>{%- endif -%}
            </div>