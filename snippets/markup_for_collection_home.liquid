{% capture listItems %}
{% for item in collections[settings.collection_home].products limit: settings.collection_home_limit %}
<div role="listitem" class="w-dyn-item">
                <div class="w-layout-grid content-grid-two-columns">
                  <div class="content-wrapper">
                    <div class="wrap-v-regular">
                      <div class="wrap-v-x-small">
                        <h3 option:text="Addison H3" class="h6-heading-2">{{ item.title }}</h3>
                        <div option:text="Addison Feature Tagline" class="overline-regular text-weight-medium">Doorless</div>
                      </div>
                      <p option:textarea="addison product p" class="subheading-small-3">Say Hello to the "ADDISON". Our unconfined design for those who prefer to come and go as they please. Recommended for those who may be a bit more on the "reconstructive" side with their mouths or nails. This beautiful open concept design provides a cozy option for them to enjoy some reprieve. For those who don't want to feel closed in but enjoy a little nook of their own!</p>
                    </div>
                    <div class="content-button-wrapper">
                      <a option:url="Addison url" data-w-id="8a844685-66d8-2a08-214a-09e1a7cf4f01" href="#" class="button-hover-50 solid is-small is-black w-inline-block">
                        <div option:text="Addison btn text" class="button-text-50 is-small">Design your own</div>
                        <div class="button-dot-50">
                          <div class="dot-50 is-small"></div>
                          <div class="button-icon-50 w-embed"><svg xmlns="http://www.w3.org/2000/svg" height="1rem" viewBox="0 0 448 512"><!-- ! Font Awesome Free 6.4.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc.  -->
                              <path d="M438.6 278.6c12.5-12.5 12.5-32.8 0-45.3l-160-160c-12.5-12.5-32.8-12.5-45.3 0s-12.5 32.8 0 45.3L338.8 224 32 224c-17.7 0-32 14.3-32 32s14.3 32 32 32l306.7 0L233.4 393.4c-12.5 12.5-12.5 32.8 0 45.3s32.8 12.5 45.3 0l160-160z" fill="currentColor"></path>
                            </svg></div>
                        </div>
                      </a>
                      <div class="paragraph-small-7" fb-tooltip-position="top" fb-tooltip-animation="shift-away" fb-tooltip-message="Ready to Go" fb-tooltip-theme="light" fb-tooltip-show-delay="500" option:text="addison extra" fb-tooltip="true" fb-tooltip-trigger="hover">No assembly required.</div>
                    </div>
                  </div>
                  <div id="w-node-_85e7b60f-e4a7-dbd4-cb90-b2525614645a-56146449" class="content-image">
                    <div id="w-node-_85e7b60f-e4a7-dbd4-cb90-b2525614645b-56146449" class="content-image-large background-brown-100"><img data-w-id="85e7b60f-e4a7-dbd4-cb90-b2525614645c" height="Auto" alt="{{ item.featured_image.alt }}" loading="lazy" src="{{ item.featured_image | img_url: 'master' }}" class="image-full-3" data-commerce-type="variation-image" srcset="{{ item.featured_image | img_url: '1920x' }} 1920w,{{ item.featured_image | img_url: '1600x' }} 1600w,{{ item.featured_image | img_url: '1366x' }} 1366w,{{ item.featured_image | img_url: '1024x' }} 1024w,{{ item.featured_image | img_url: '768x' }} 768w,{{ item.featured_image | img_url: '640x' }} 640w"></div>
                  </div>
                </div>
              </div>
{%- endfor -%}
{% endcapture %}
{% assign items_length = listItems | strip | size %}
<div class="w-dyn-list" udy-collection="product">
            {%- unless items_length == 0 -%}<div role="list" class="w-dyn-items">
              {{ listItems }}
            </div>{%- endunless -%}
            {%- if items_length == 0 -%}<div class="w-dyn-empty">
              <div>No items found.</div>
            </div>{%- endif -%}
          </div>